void mapEditor(float x, float y, int type)
{
  push();
  float tileWidth = (width/2)/mapWidth;
  float tileHeight = (height)/mapHeight;
  int mapX = floor(x/tileWidth);
  int mapY = floor(y/tileHeight);
  pop();
  
  println(mapX, mapY);
  
  if(mapX < mapWidth && mapX >= 0 && mapY < mapHeight && mapY >= 0)
  {
    map[mapX][mapY] = type;
  }
}
